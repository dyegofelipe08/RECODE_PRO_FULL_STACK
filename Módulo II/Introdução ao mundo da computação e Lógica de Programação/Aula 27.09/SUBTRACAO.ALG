Algoritmo "subtracao"

Var
a, b, res : inteiro

Inicio
      escreval ("Olá! Vamos fazer uma conta de subtração.")
      escreval ("Digite o primeiro número: ")
      leia (a)
      escreval ("Digite o segundo número: ")
      leia (b)
      res <- (a-b)
      escreval ("O resultado da subtração entre ",a, " e ", b, " é: ", res)


Fimalgoritmo