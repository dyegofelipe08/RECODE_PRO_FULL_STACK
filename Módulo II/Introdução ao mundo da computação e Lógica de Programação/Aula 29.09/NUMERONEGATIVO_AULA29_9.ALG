Algoritmo "numeroNegativo_aula29_9"

Var
n : inteiro

Inicio
   escreva ("Digite um número inteiro: ")
   leia (n)
   se (n<0) entao
      escreva ("O número digitado é nagativo!")
   senao
        se (n>0) entao
           escreva ("O número digitado é positivo!")
           senao
           escreva ("O número é igual a zero!")
        fimse
   fimse

Fimalgoritmo