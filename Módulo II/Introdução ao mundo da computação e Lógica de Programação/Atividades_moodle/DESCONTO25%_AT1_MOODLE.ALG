Algoritmo "desconto25%_at1_moodle"

Var

preco_lido, preco_final: real

Inicio

      escreva("Digite o preço do produto: ")
      leia(preco_lido)
      preco_final<-(preco_lido*0.75)
      
      escreval("Parabéns! Você obteve um desconto de 25% no valor deste produto.")
      escreval("Você pagará apenas, R$",preco_final)
      



Fimalgoritmo