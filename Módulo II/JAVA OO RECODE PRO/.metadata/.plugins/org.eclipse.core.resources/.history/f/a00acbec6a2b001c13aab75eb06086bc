package Exemplos;

public class ExTryCatch01 {

	public static void main(String[] args) {
		
		try {
			int [] numeros = {1,2,3};
			System.out.println(numeros[10]);
		}catch (Exception e){
			System.out.println("Algo deu errado!");
		}finally {
			System.out.println("O try catch terminou.");
		}

	}

}
